{
  "name": "Edmunds.com HTTP API - Dealers",
  "baseUrl": "http://api.edmunds.com",
  "operations": {
    "abstractService": {
      "httpMethod": "GET",
      "parameters": {
        "api_key": {
          "location": "query",
          "description": "API key",
          "required": true
        },
        "fmt": {
          "location": "query",
          "required": true
        }
      }
    },
    "getDealerDetails": {
      "extends": "abstractService",
      "uri": "/api/dealer/v2/dealers/{dealerId}",
      "responseClass": "Dealer",
      "summary": "Return a specific dealer",
      "parameters": {
        "dealerId": {
          "location": "uri",
          "required": true
        },
        "view": {
          "location": "query",
          "required": false
        }
      }
    },
    "searchDealers": {
      "extends": "abstractService",
      "uri": "/api/dealer/v2/dealers",
      "responseClass": "dealerHolderModel",
      "summary": "Return a filtered list of dealers",
      "parameters": {
        "zipcode": {
          "location": "query",
          "required": true
        },
        "radius": {
          "location": "query",
          "required": false
        },
        "make": {
          "location": "query",
          "required": false
        },
        "state": {
          "location": "query",
          "required": false
        },
        "pageNum": {
          "location": "query",
          "required": false
        },
        "pageSize": {
          "location": "query",
          "required": false
        },
        "sortby": {
          "location": "query",
          "required": false
        },
        "keyword": {
          "location": "query",
          "required": false
        },
        "view": {
          "location": "query",
          "required": false
        }
      }
    },
    "getDealerCount": {
      "extends": "abstractService",
      "uri": "/api/dealer/v2/dealers/count",
      "responseClass": "dealerCountModel",
      "summary": "Return a count of a filtered list of dealers",
      "parameters": {
        "zipcode": {
          "location": "query",
          "required": true
        },
        "radius": {
          "location": "query",
          "required": false
        },
        "make": {
          "location": "query",
          "required": false
        },
        "state": {
          "location": "query",
          "required": false
        },
        "keyword": {
          "location": "query",
          "required": false
        }
      }
    }
  },
  "models": {
    "dealerHolderModel": {
      "type": "object",
      "properties": {
        "dealers": {
          "$ref": "dealerHolder"
        }
      }
    },
    "dealerHolder": {
      "location": "json",
      "type": "array",
      "items": {
        "$ref": "Dealer"
      }
    },
    "dealerCountModel": {
      "location": "json",
      "type": "object",
      "properties": {
        "dealersCount": {
          "type": "number"
        }
      }
    },
    "Dealer": {
      "location": "json",
      "type": "object",
      "properties": {
        "dealerId": {
          "type": "string"
        },
        "address": {
          "$ref": "Address"
        },
        "name": {
          "type": "string"
        },
        "niceName": {
          "type": "string"
        },
        "type": {
          "type": "string"
        },
        "make": {
          "type": "string"
        },
        "operations": {
          "type": "object",
          "additionalProperties": true
        },
        "active": {
          "type": "boolean"
        },
        "franchises": {
          "type": "array",
          "items": {
            "type": "object",
            "additionalProperties": true,
            "properties": {
              "name": {
                "type": "string"
              },
              "address": {
                "$ref": "Address"
              },
              "active": {
                "type": "boolean"
              },
              "franchiseId": {
                "type": "string"
              },
              "make": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "niceName": {
                    "type": "string"
                  }
                }
              },
              "tier": {
                "type": "object",
                "additionalProperties": true
              },
              "contactInfo": {
                "$ref": "ContactInfo"
              },
              "status": {
                "type": "string"
              },
              "lpStatus": {
                "type": "string"
              },
              "ppStatus": {
                "type": "string"
              },
              "upStatus": {
                "type": "string"
              },
              "cpStatus": {
                "type": "string"
              },
              "premier": {
                "type": "boolean"
              }
            }
          }
        },
        "contactInfo": {
          "$ref": "ContactInfo"
        },
        "reviews": {
          "type": "object",
          "properties": {
            "sales": {
              "type": "object",
              "additionalProperties": true,
              "properties": {
                "overallRating": {
                  "type": "number"
                },
                "count": {
                  "type": "number"
                },
                "recommendedCount": {
                  "type": "number"
                },
                "notRecommendedCount": {
                  "type": "number"
                }
              }
            },
            "service": {
              "type": "object",
              "additionalProperties": true,
              "properties": {
                "overallRating": {
                  "type": "number"
                },
                "count": {
                  "type": "number"
                },
                "recommendedCount": {
                  "type": "number"
                },
                "notRecommendedCount": {
                  "type": "number"
                }
              }
            }
          }
        },
        "repairshops": {
          "type": "array",
          "items": {
            "type": "object",
            "additionalProperties": true,
            "properties": {
              "name": {
                "type": "string"
              },
              "address": {
                "$ref": "Address"
              },
              "active": {
                "type": "boolean"
              },
              "repairshopId": {
                "type": "string"
              },
              "make": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "niceName": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "status": {
          "type": ["array", "any"],
          "items": {
            "type": "string"
          }
        },
        "type": {
          "type": "string"
        }
      }
    },
    "Address": {
      "type": "object",
      "properties": {
        "street": {
          "type": "string"
        },
        "apartment": {
          "type": "string"
        },
        "city": {
          "type": "string"
        },
        "stateCode": {
          "type": "string"
        },
        "stateName": {
          "type": "string"
        },
        "county": {
          "type": "string"
        },
        "country": {
          "type": "string"
        },
        "zipcode": {
          "type": "string"
        },
        "latitude": {
          "type": "string"
        },
        "longitude": {
          "type": "string"
        }
      }
    },
    "ContactInfo": {
      "type": "object",
      "properties": {
        "website": {
          "type": "string"
        },
        "phone": {
          "type": "string"
        }
      },
      "additionalProperties": true
    }
  }
}
