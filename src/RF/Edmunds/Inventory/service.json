{
  "name": "Edmunds.com HTTP API - Inventory",
  "baseUrl": "http://api.edmunds.com",
  "operations": {
    "abstractService": {
      "httpMethod": "GET",
      "parameters": {
        "api_key": {
          "location": "query",
          "description": "API key",
          "required": true
        },
        "fmt": {
          "location": "query",
          "required": true
        }
      }
    },
    "getByVin": {
      "extends": "abstractService",
      "uri": "/api/inventory/v1/lookup",
      "responseClass": "inventoryHolderModel",
      "summary": "Look up a car available for sale by its VIN, zip code and range",
      "parameters": {
        "vin": {
          "location": "query",
          "required": true
        },
        "zipcode": {
          "location": "query",
          "required": true
        },
        "range": {
          "location": "query",
          "required": true
        }
      }
    },
    "getAllByDealerId": {
      "extends": "abstractService",
      "uri": "/api/inventory/v1/getall",
      "responseClass": "inventoryDealerHolderModel",
      "summary": "Get cars available for sale at a specific dealership by providing the dealership ID",
      "parameters": {
        "dealerId": {
          "location": "query",
          "required": true
        },
        "types": {
          "location": "query",
          "required": true,
          "type": "string",
          "enum": [ "ALL", "NEW", "USED", "CPO" ]
        },
        "sort": {
          "location": "query",
          "required": false
        },
        "pageNumber": {
          "location": "query",
          "required": false
        },
        "pageSize": {
          "location": "query",
          "required": false
        },
        "basicFilter": {
          "location": "query",
          "required": false
        }
      }
    },
    "getByZip": {
      "extends": "abstractService",
      "uri": "/api/inventory/v1/getall",
      "responseClass": "inventoryDealerHolderModel",
      "summary": "Get cars available for sale within a specific area by supplying a five-digit ZIP code.",
      "parameters": {"zipcode": {
        "required": true,
        "location": "query"
      },
        "range": {
          "required": true,
          "location": "query"
        },
        "radius": {
          "required": true,
          "location": "query"
        },
        "make": {
          "required": true,
          "location": "query"
        },
        "types": {
          "required": false,
          "location": "query"
        },
        "sort": {
          "required": false,
          "location": "query"
        },
        "pageNumber": {
          "required": false,
          "location": "query"
        },
        "pageSize": {
          "required": false,
          "location": "query"
        },
        "pagenum": {
          "required": false,
          "location": "query"
        },
        "pagesize": {
          "required": false,
          "location": "query"
        },
        "basicFilter": {
          "required": false,
          "location": "query"
        },
        "model": {
          "required": false,
          "location": "query"
        },
        "submodel": {
          "required": false,
          "location": "query"
        },
        "priceRange": {
          "required": false,
          "location": "query"
        },
        "dealerId": {
          "required": false,
          "location": "query"
        }
      }
    }
  },
  "models": {
    "inventoryHolderModel": {
      "type": "object",
      "location": "json",
      "properties": {
        "resultsList": {
          "type": "array",
          "location": "json",
          "items": {
            "type": "object",
            "location": "json",
            "properties": {
              "year": {
                "type": "string",
                "location": "json"
              }
            }
          }
        }
      }
    },
    "inventoryDealerHolderModel": {
      "type": "object",
      "location": "json",
      "properties": {
        "totalCount": {
          "type": "number"
        },
        "resultsList": {
          "$ref": "inventoryHolder"
        }
      }
    },
    "inventoryHolder": {
      "type": "array",
      "location": "json",
      "items": {
        "$ref": "Inventory"
      }
    },
    "Inventory": {
      "type": "object",
      "location": "json",
      "properties": {
        "year": {
          "location": "json",
          "type": "number"
        },
        "features": {
          "type": "array",
          "items": {
            "type": "any"
          }
        },
        "options": {
          "type": "array",
          "items": {
            "type": "any"
          }
        },
        "make": {
          "type": "string"
        },
        "model": {
          "type": "string"
        },
        "bodyType": {
          "type": "string"
        },
        "trim": {
          "type": "string"
        },
        "modelYearId": {
          "type": "number"
        },
        "styleId": {
          "type": "number"
        },
        "transmission": {
          "type": "string"
        },
        "modelLinkCode": {
          "type": "string"
        },
        "submodelId": {
          "type": "number"
        },
        "combinedMpg": {
          "type": "number"
        },
        "styleName": {
          "type": "string"
        },
        "submodel": {
          "type": "string"
        },
        "inventoryType": {
          "type": "string"
        },
        "engineSize": {
          "type": "number"
        },
        "driveTrain": {
          "type": "string"
        },
        "franchiseId": {
          "type": "number"
        },
        "exteriorColor": {
          "type": "string"
        },
        "interiorColor": {
          "type": "string"
        },
        "carfaxes": {
          "type": "array",
          "items": {
            "type": "any"
          }
        },
        "hasBuildDataAndFullyMatched": {
          "type": "boolean"
        },
        "vin": {
          "type": "string"
        },
        "mileage": {
          "type": "number"
        },
        "inventoryId": {
          "type": "number"
        },
        "cityMpg": {
          "type": "number"
        },
        "hwyMpg": {
          "type": "number"
        },
        "msrpPrice": {
          "type": "number"
        },
        "inventoryPrice": {
          "type": "number"
        },
        "stockNumber": {
          "type": "string"
        },
        "vehicleComments": {
          "type": "string"
        },
        "exteriorGenericColor": {
          "type": "string"
        },
        "interiorGenericColor": {
          "type": "string"
        },
        "guaranteedPrice": {
          "type": "number"
        },
        "gpexperiationDate": {
          "type": "string"
        },
        "tmvinventoryPrice": {
          "type": "number"
        },
        "tmvdealerCash": {
          "type": "number"
        },
        "tmvcustomerIncentives": {
          "type": "number"
        },
        "invoicePrice": {
          "type": "number"
        },
        "dealerInPDP": {
          "type": "boolean"
        },
        "dealerName": {
          "type": "string"
        },
        "dealerPhone": {
          "type": "string"
        },
        "dealerLatitude": {
          "type": "number"
        },
        "dealerLongitude": {
          "type": "number"
        },
        "dealerDistance": {
          "type": "number"
        },
        "dealerServiceRating": {
          "type": "number"
        },
        "dealerSaleRating": {
          "type": "number"
        },
        "countMatchedVehicles": {
          "type": "number"
        },
        "countAllVehicles": {
          "type": "number"
        },
        "dealerLocationId": {
          "type": "number"
        },
        "dealerSalesReviewsCount": {
          "type": "number"
        },
        "dealerServiceReviewsCount": {
          "type": "number"
        },
        "dealerMake": {
          "type": "string"
        },
        "f34PhotoUrlsE": {
          "type": "string"
        },
        "f34PhotoUrlsSE": {
          "type": "string"
        },
        "f34PhotoUrlsT": {
          "type": "string"
        },
        "photoUrlsE": {
          "type": "array",
          "items": {
            "type": "any"
          }
        },
        "photoUrlsST": {
          "type": "array",
          "items": {
            "type": "any"
          }
        },
        "photoUrlsT": {
          "type": "array",
          "items": {
            "type": "any"
          }
        },
        "tmvPrice": {
          "type": "number"
        },
        "premierDealer": {
          "type": "boolean"
        }
      },
      "additionProperties": true
    }
  }
}
