            }
          }
        },
        "trim": {
          "location": "json",
          "type": "object",
          "properties": {
            "name": {
              "location": "json",
              "type": "string"
            },
            "niceName": {
              "location": "json",
              "type": "string"
            }
          }
        },
        "specification": {
          "location": "json",
          "type": "object",
          "properties": {
            "elements": {
              "location": "json",
              "type": "array"
            },
            "type": {
              "location": "json",
              "type": "string"
            }  
          }
        }
      },
      "additionalProperties": true
    }

  }
}