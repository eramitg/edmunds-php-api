        },
        "makeName": {
          "location": "json",
          "type": "string"
        },
        "makeNiceName": {
          "location": "json",
          "type": "string"
        },
        "modelId": {
          "location": "json",
          "type": "string"
        },
        "modelName": {
          "location": "json",
          "type": "string"
        },
        "modelNiceName": {
          "location": "json",
          "type": "string"
        },
        "id": {
          "location": "json",
          "type": "integer"
        },
        "styles": {
          "location": "json",
          "type": "array",
          "items": {
            "$ref": "Style"
          }
        },
        "newDefaultStyles": {
          "location": "json",
          "type": "object",
          "additionalProperties": true
        },
        "usedDefaultStyle": {
          "location": "json",
          "type": "object",
          "additionalProperties": true
        },
        "equipment": {
          "location": "json",
          "type": "array",
          "items": {
            "location": "json",
            "type": "object",
            "additionalProperties": true
          }
        },
        "subModels": {
          "location": "json",
          "type": "array",
          "items": {
            "$ref": "SubModel"
          }
        },
        "midYear": {
          "location": "json",
          "type": "boolean"
        },
        "year": {
          "location": "json",
          "type": "integer"
        },
        "dateAvailable": {
          "location": "json",
          "type": "any"
        },
        "model": {
          "location": "json",
          "$ref": "Model"
        },
        "name": {
          "location": "json",
          "type": "string"
        },
        "attributeGroups": {
          "location": "json",
          "type": "object",
          "additionalProperties": true
        },
        "categories": {
          "location": "json",
          "type": "object",
          "additionalProperties": true
        },
        "publicationStates": {
          "location": "json",
          "type": "array",
          "items": {
            "location": "json",
            "type": "string"
          }
        }
      }
    },
    "Style": {
