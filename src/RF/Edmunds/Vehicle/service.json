        },
        "year": {
          "location": "json",
          "type": "integer"
        },
        "makeName": {
          "location": "json",
          "type": "string"
        },
        "makeNiceName": {
          "location": "json",
          "type": "string"
        },
        "modelId": {
          "location": "json",
