{
  "name": "Edmunds.com HTTP API - Vehicles",
  "apiVersion": "1.0",
  "operations": {
    "AbstractService": {
      "httpMethod": "GET",
      "parameters": {
        "version": {
          "location": "uri",
          "description": "API version",
          "required": true
        },
        "api_key": {
          "location": "query",
          "description": "API version",
          "required": true
        },
        "fmt": {
          "location": "query",
          "required": true
        }
      }
    },
    "Tools.VinDecoder": {
      "extends": "AbstractService",
      "uri": "/{version}/api/toolsrepository/vindecoder",
      "responseClass": "styleHolder",
      "summary": "Return a style object for a given vin",
      "parameters": {
        "vin": {
          "location": "query",
          "required": true
        }
      }
    },
    "Make.FindAll": {
      "extends": "AbstractService",
      "uri": "/{version}/api/vehicle/makerepository/findall",
      "responseClass": "makeHolder",
      "summary": "Find All Makes"
    },
    "Make.FindById": {
      "extends": "AbstractService",
      "uri": "/{version}/api/vehicle/makerepository/findbyid",
      "responseClass": "makeHolder",
      "parameters": {
        "id": {
          "location": "query",
          "require": true
        }
      }
    },
    "Make.FindFutureMakes": {
      "extends": "AbstractService",
      "uri": "/{version}/api/vehicle/makerepository/findfuturemakes",
      "responseClass": "makeHolder"
    },
    "Make.FindMakeByName": {
      "extends": "AbstractService",
      "uri": "/{version}/api/vehicle/makerepository/findmakebyname",
      "responseClass": "makeHolder",
      "parameters": {
        "name": {
          "location": "query",
          "required": true
        }
      }
    },
    "Make.FindMakesByModelYear": {
      "extends": "AbstractService",
      "uri": "/{version}/api/vehicle/makerepository/findmakesbymodelyear",
      "responseClass": "makeHolder",
      "parameters": {
        "year": {
          "location": "query",
          "required": true
        }
      }
    },
    "Make.FindMakesByPublicationState": {
      "extends": "AbstractService",
      "uri": "/{version}/api/vehicle/makerepository/findmakesbypublicationstate",
      "responseClass": "makeHolder",
      "parameters": {
        "state": {
          "location": "query",
          "required": true,
          "type": "enum:new,used,future"
        }
      }
    },
    "Make.FindNewAndUsed": {
      "extends": "AbstractService",
      "uri": "/{version}/api/vehicle/makerepository/findnewandused",
      "responseClass": "makeHolder"
    },
    "Make.FindNewAndUsedMakesByModelYear": {
      "extends": "AbstractService",
      "uri": "/{version}/api/vehicle/makerepository/findnewandusedmakesbymodelyear",
      "responseClass": "makeHolder",
      "parameters": {
        "year": {
          "location": "query",
          "required": true
        }
      }
    },
    "Make.FindNewMakes": {
      "extends": "AbstractService",
      "uri": "/{version}/api/vehicle/makerepository/findnewmakes",
      "responseClass": "makeHolder"
    },
    "Make.FindUsedMakes": {
      "extends": "AbstractService",
      "uri": "/{version}/api/vehicle/makerepository/findusedmakes",
      "responseClass": "makeHolder"
    }
  },
  "models": {
    "makeHolder": {
      "type": "array",
      "items": {
        "$ref": "Make"
      }
    },
    "modelYearHolder": {
      "type": "array",
      "items": {
        "$ref": "ModelYear"
      }
    },
    "styleHolder": {
      "type": "array",
      "items": {
        "$ref": "Style"
      }
    },
    "Make": {
      "location": "json",
      "type": "object",
      "properties": {
        "id": {
          "location": "json",
          "type": "integer"
        },
        "models": {
          "location": "json",
          "type": "array",
          "items": {
            "$ref": "Model"
          }
        },
        "name": {
          "location": "json",
          "type": "string"
        },
        "niceName": {
          "location": "json",
          "type": "string"
        },
        "attributeGroups": {
          "location": "json",
          "type": "object",
          "additionalProperties": true
        }
      }
    },
    "Model": {
      "location": "json",
      "type": "object",
      "properties": {
        "link": {
          "location": "json",
          "type": "string"
        },
        "id": {
          "location": "json",
          "type": "string"
        },
        "name": {
          "location": "json",
          "type": "string"
        } 
      }
    },
    "ModelYear": {
      "location": "json",
      "type": "object",
